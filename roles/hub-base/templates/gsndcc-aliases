#  Aliases common to user iris

alias m more
alias mv '/bin/mv -i'
alias su 'su -'

alias del	/bin/rm -i
alias egrep	egrep --color=auto
alias fgrep	fgrep --color=auto
alias grep	grep --color=auto
alias h	history
alias j	jobs
alias ls    /bin/ls -F
alias l.	ls -d .* --color=auto
alias ll	ls -l --color=auto
alias la	ls -la --color=auto
alias m	more
alias mon	nrts_mon
alias mv	/bin/mv -i
alias pathadd	/usr/local/pathadd.csh
alias pid 'ps -aux | egrep "(\!*|USER)" | egrep -v "(grep| ps -)"'
alias pib 'ps -ax -o user,pid,%cpu,%mem,state,start,command | grep "\!*" | egrep -v "(grep| ps -)" | sort -k 7'
alias vi	vim
